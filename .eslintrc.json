{
  "extends": ["eslint:recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "worker": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
